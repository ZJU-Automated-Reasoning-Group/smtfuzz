#!/usr/bin/env python3
"""
SMTFuzz Generation-based Fuzzing Tool

This script provides a command-line interface for generation-based SMT fuzzing.
"""

import sys
import os

# Add the parent directory to the path so we can import smtfuzz
sys.path.insert(0, os.path.join(os.path.dirname(__file__), '..'))

from smtfuzz.runner.gen_runner import main

if __name__ == "__main__":
    main() 