#!/usr/bin/env python3
"""
SMTFuzz Runner Command-Line Interface

This script provides advanced fuzzing modes for SMTFuzz:
- seed: Seed-based fuzzing using existing SMT files
- gen: Generation-based fuzzing creating new SMT formulas

For the original SMT formula generator, use the smtfuzz command.
"""

import sys
import os

# Add the parent directory to the path so we can import smtfuzz
sys.path.insert(0, os.path.join(os.path.dirname(__file__), '..'))


def main():
    from smtfuzz.runner_cli import main as runner_main
    runner_main()


if __name__ == "__main__":
    main() 